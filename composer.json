{
  "require": {
    "slim/slim": "^4.0",
    "guzzlehttp/guzzle": "^7.0",
    "aws/aws-sdk-php": "^3.0",
    "open-telemetry/sdk": "2.x-dev",
    "open-telemetry/exporter-otlp": "2.x-dev",
    "open-telemetry/opentelemetry-auto-slim": "dev-main",
    "open-telemetry/opentelemetry-auto-psr18": "dev-main",
    "open-telemetry/opentelemetry-auto-guzzle": "dev-main",
    "nyholm/psr7": "^1.8@dev"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "MyApp\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "tbachert/spi": true
    }
  }
}